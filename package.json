{
  "name": "@eljs/node-utils",
  "version": "0.4.3",
  "description": "a collection of node utility functions",
  "main": "dist/node-utils.js",
  "files": [
    "lib"
  ],
  "types": "dist/node-utils.d.ts",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "lint": "eslint 'src/**/*.@(js|ts)' --fix",
    "format": "prettier --write 'src/**/*.@(js|ts)'",
    "test": "npm run test:once -- --watch",
    "test:once": "jest --runInBand --colors --forceExit",
    "coverage": "codecov",
    "release": "node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chnliquan/node-utils"
  },
  "keywords": [
    "node",
    "utility"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/co": "^4.6.3",
    "@types/download": "^8.0.1",
    "@types/ejs": "^3.1.0",
    "@types/ini": "^1.3.31",
    "@types/inquirer": "^8.2.0",
    "@types/mkdirp": "^1.0.2",
    "@types/parse-json": "^4.0.0",
    "@types/read": "0.0.29",
    "@types/require-from-string": "^1.2.1",
    "@types/rimraf": "^2.0.3",
    "@types/signale": "^1.4.1",
    "@types/uuid": "^8.3.4",
    "@types/which": "^2.0.1",
    "chalk": "^4.1.2",
    "co": "^4.6.0",
    "download": "^8.0.0",
    "ejs": "^3.1.6",
    "execa": "^5.1.1",
    "ini": "^2.0.0",
    "inquirer": "^8.2.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "ora": "^5.4.1",
    "parse-json": "^5.0.0",
    "read": "^1.0.7",
    "require-from-string": "^2.0.2",
    "rimraf": "^3.0.0",
    "urllib": "^2.36.1",
    "uuid": "^8.3.2",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@eljs/release": "0.5.0",
    "@microsoft/api-extractor": "^7.19.4",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/fs-extra": "^9.0.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "codecov": "^3.8.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "execa": "^5.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "minimist": "^1.2.5",
    "prettier": "^2.5.1",
    "rollup": "^2.69.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "tslib": "^2.3.1",
    "typescript": "4.5.5"
  },
  "author": "chnliquan",
  "publishConfig": {
    "registry": "https://registry.npmjs.org "
  },
  "license": "MIT"
}
